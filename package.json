{
  "name": "geodiag-backend",
  "version": "2.0.3",
  "description": "Backend API for the GeoDiag wheel alignment application.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest --runInBand",
    "test:integration": "node --experimental-vm-modules ./node_modules/jest/bin/jest --runInBand tests/integration",
    "test:e2e": "node --experimental-vm-modules ./node_modules/jest/bin/jest --runInBand tests/e2e",
    "test:handles": "node --experimental-vm-modules ./node_modules/jest/bin/jest --runInBand --detectOpenHandles",
    "test:coverage": "node --experimental-vm-modules ./node_modules/jest/bin/jest --runInBand --coverage",
    "lint": "eslint . --ext .js",
    "migrate": "node-pg-migrate",
    "worker": "node src/worker/index.js",
    "tree": "tree /f | findstr /v /i \"node_modules coverage dist .git package-lock.json .d.ts\"",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\""
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dataloader": "^2.2.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.0.1",
    "graphql": "^16.8.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-query-complexity": "^1.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.1",
    "json-rules-engine": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "node-pg-migrate": "^7.5.2",
    "nodemailer": "^7.0.5",
    "pdfkit": "^0.17.1",
    "pg": "^8.12.0",
    "pg-boss": "^10.3.2",
    "pino": "^9.7.0",
    "pino-pretty": "^13.1.1",
    "stripe": "^18.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.13.0",
    "@eslint/markdown": "^6.6.0",
    "@flydotio/dockerfile": "^0.7.10",
    "eslint": "^9.30.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-markdown": "^5.1.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "nodemailer-mock": "^2.0.9",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0"
  },
  "keywords": [
    "geodiag",
    "api",
    "graphql",
    "nodejs",
    "express"
  ],
  "author": "Assistant Sinistre",
  "license": "ISC"
}
